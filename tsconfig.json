{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "strict": false,
    "jsx": "react-native",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@assets/*": ["assets/*"],
      "@styles/*": ["styles/*"],
      "@hooks/*": ["hooks/*"],
      "@utils/*": ["utils/*"],
      "@store/*": ["store/*"],
      "@clientTypes/*": ["types/*"],
      "@const/*": ["const/*"],
      "@language/*": ["language/*"],
      "@api/*": ["api/*"],
      "@/*": ["./*"],
    },
  },
}
